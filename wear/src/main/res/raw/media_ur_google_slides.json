{
  "title": "Google Slides",
  "background": "bkg_google_slides",
  "media_actions": [
    {
      "text": "Previous",
      "package": "android",
      "drawable": "ic_media_previous",
      "intent": "unified",
      "uri": "ur://intent/remote:Unified.SlideShow/action:previous",
      "onGesture": "up",
      "target": "phone"
    },
    {
      "text": "Next",
      "package": "android",
      "drawable": "ic_media_next",
      "intent": "unified",
      "uri": "ur://intent/remote:Unified.SlideShow/action:next",
      "onGesture": "down",
      "target": "phone"
    },
    {
      "text": "Start show",
      "package": "android",
      "drawable": "ic_media_play",
      "intent": "unified",
      "uri": "ur://intent/remote:Unified.GoogleSlides/action:show_start",
      "onGesture": "enter",
      "target": "phone"
    },
    {
      "text": "End show",
      "package": "android",
      "drawable": "ic_media_pause",
      "intent": "unified",
      "uri": "ur://intent/remote:Unified.SlideShow/action:show_end",
      "onGesture": "home",
      "target": "phone"
    }
  ]
}