{
  "title": "Keynote Slides",
  "background": "bkg_google_slides",
  "gesture_actions": [
    {
      "text": "Previous",
      "package": "android",
      "drawable": "ic_media_previous",
      "onGesture": "up",
      "target": "phone",
      "intent" : "ur://intent/remote:Unified.Keynote/action:previous"
    },
    {
      "text": "Next",
      "package": "android",
      "drawable": "ic_media_next",
      "onGesture": "down",
      "target": "phone",
      "intent" : "ur://intent/remote:Unified.Keynote/action:next"
    },
    {
      "text": "Start show",
      "package": "android",
      "drawable": "ic_media_play",
      "onGesture": "enter",
      "target": "phone",
      "intent" : "ur://intent/remote:Unified.Keynote/action:start_show"
    },
    {
      "text": "End show",
      "package": "android",
      "drawable": "ic_media_pause",
      "onGesture": "home",
      "target": "phone",
      "intent" : "ur://intent/remote:Unified.Keynote/action:show_end"
    }
  ]
}