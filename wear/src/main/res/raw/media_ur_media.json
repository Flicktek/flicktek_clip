{
  "title": "Media",
  "background": "bkg_music",
  "media_actions": [
    {
      "text": "Previous",
      "package": "android",
      "drawable": "ic_media_previous",
      "intent": "unified",
      "uri": "ur://intent/remote:Unified.Media/action:volume_down",
      "onGesture": "up",
      "target": "phone"
    },
    {
      "text": "Next",
      "package": "android",
      "drawable": "ic_media_next",
      "intent": "unified",
      "uri": "ur://intent/remote:Unified.Media/action:volume_up",
      "onGesture": "down",
      "target": "phone"
    },
    {
      "text": "Start show",
      "package": "android",
      "drawable": "ic_media_play",
      "intent": "unified",
      "uri": "ur://intent/remote:Unified.Media/action:play_pause",
      "onGesture": "enter",
      "target": "phone"
    },
    {
      "text": "End show",
      "package": "android",
      "drawable": "ic_media_pause",
      "intent": "unified",
      "uri": "ur://intent/remote:Unified.Media/action:stop",
      "onGesture": "home",
      "target": "phone"
    }
  ]
}